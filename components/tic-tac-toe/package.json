{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "description": "Returns an tic-tac-toe embed",
  "main": "dist/index.js",
  "scripts": {
    "webcm": "cd ../../ && npx webcm",
    "watch": "nodemon -e ts,js,css,html -x \"npm-run-all \"build\" \"webcm\"\" ",
    "dev": "nodemon npm run build && cd ../../ && npx webcm",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "bundle": "node esbuild.js",
    "build": "npm run test && npm run lint && npm run typecheck && npm run bundle",
    "typecheck": "tsc --project tsconfig.build.json --noEmit",
    "test": "vitest run --globals",
    "test:dev": "vitest --globals"
  },
  "keywords": [
    "webcm",
    "managed-components",
    "tic-tac-toe"
  ],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@managed-components/types": "^1.3.14",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "esbuild": "^0.20.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.11.0",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.11.0",
    "vitest": "^1.3.1"
  }
}
